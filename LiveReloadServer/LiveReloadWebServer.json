{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning"
    }
  },
  "LiveReload": {
    "WebSocketUrl": "/__livereload",
    "AdditionalMimeMappings": {
      ".dll": "application/octet-stream"
    }
  },
  "Port": 5200,
  "UseSsl": false,
  "Host": "localhost",
  "DefaultFiles": "index.html,default.htm",
  "Extensions": ".cshtml,.css,.js,.htm,.html,.ts,.razor",
  "UseLiveReload": true,
  "ShowUrls": true,
  "OpenBrowser": true,
  "FolderNotFoundFallbackPath":  "/index.html",
  "UseRazor": false,
  "UseMarkdown": false,
  "CopyMarkdownResources": false,
  "MarkdownTemplate": "",
  "MarkdownTheme": "github",
  "MarkdownSyntaxTheme": "github"
}
